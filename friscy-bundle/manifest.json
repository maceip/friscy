{
  "version": 1,
  "image": "friscy-alpine",
  "entrypoint": ["/bin/sh"],
  "rootfs": "./rootfs.tar",
  "workdir": "/root",
  "env": [
    "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "HOME=/root",
    "TERM=xterm-256color"
  ],
  "aot": [],
  "examples": {
    "nodejs": {
      "rootfs": "./nodejs.tar",
      "entrypoint": ["/usr/bin/node", "--jitless", "--max-old-space-size=256", "-i"],
      "image": "friscy-nodejs"
    },
    "server": {
      "rootfs": "./goserver.tar",
      "entrypoint": ["/bin/echo_server"],
      "image": "friscy-goserver"
    }
  }
}
